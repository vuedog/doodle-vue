{
  "name": "doodle-vue",
  "version": "1.0.7",
  "description": "Doodle with vue [For learning vue.js]",
  "main": "dist/vd-doodle.js",
  "module": "dist/vd-doodle.js",
  "scripts": {
    "build": "rimraf ./dist && webpack",
    "watch": "webpack --watch",
    "lint": "eslint --ext .js,.vue src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run unit",
    "unit": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuedog/doodle-vue.git"
  },
  "keywords": [],
  "author": "Runrioter <runrioter@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^4.2.2",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^21.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-vue": "^0.5.6",
    "rimraf": "^3.0.2",
    "vue": "^2.4.4",
    "vue-loader": "^15.9.3",
    "vue-server-renderer": "^2.4.4",
    "vue-template-compiler": "^2.4.2",
    "vue-test-utils": "git+https://github.com/vuejs/vue-test-utils.git",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
